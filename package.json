{
  "name": "webapi-tutorial",
  "version": "0.0.0",
  "description": "Web API 教程",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "prepush": "npm update",
    "build": "loppo --site \"Web API 教程\" --id webapi --theme wangdoc",
    "build-and-commit": "npm run build && npm run commit",
    "commit": "gh-pages --dist dist --dest dist/webapi --branch master --repo git@github.com:wangdoc/website.git",
    "chapter": "loppo chapter",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wangdoc/webapi-tutorial.git"
  },
  "keywords": [
    "Tutorial",
    "Web",
    "wangdoc",
    "document"
  ],
  "author": "Ruan Yifeng",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "loppo": "^0.6.11",
    "loppo-theme-wangdoc": "^0.1.8"
  }
}
